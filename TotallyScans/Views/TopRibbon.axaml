<UserControl xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        Height="60"
        Background="#44000000"
        HorizontalAlignment="Stretch"
        x:Class="TotallyScans.MenuRibbon">
    
        <StackPanel Margin="30,0,0,0" x:Name="RibbonPanel" Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center">
                <Image x:Name="RibbonImage" Width="100" Height="0" Margin="0,0,30,0" Stretch="Uniform"></Image>
        </StackPanel>
        
</UserControl>
